{
  "name": "@superflycss/component-header",
  "version": "1.1.18",
  "description": "Header component",
  "main": "src/main/css/index.css",
  "scripts": {
    "update": "ncu -a && npm i",
    "build": "gulp clean && gulp build:css",
    "test": "gulp clean && gulp build:css && gulp test:css",
    "clean": "gulp clean",
    "watch:test": "onchange 'src/test/html/*.html' 'src/test/css/*.css' 'src/main/css/*.css' -- npm test",
    "serve": "lite-server",
    "deploy": "npm run test && gulp deploy:test:css && gulp deploy:test:html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superflycss/component-header.git"
  },
  "keywords": [
    "css",
    "superflycss",
    "header",
    "component"
  ],
  "author": "Ole Ersoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/superflycss/component-header/issues"
  },
  "homepage": "https://github.com/superflycss/component-header#readme",
  "devDependencies": {
    "@superflycss/component-body": "^1.0.1",
    "@superflycss/component-display": "^1.0.2",
    "@superflycss/component-test": "^2.0.22",
    "@superflycss/foundation": "^1.0.0",
    "@superflycss/task-build": "^1.0.5",
    "@superflycss/task-clean": "^1.0.2",
    "@superflycss/task-deploy": "^2.0.4",
    "@superflycss/task-lint": "^1.0.0",
    "@superflycss/task-test": "^1.0.1",
    "@superflycss/utilities-colors": "^2.1.1",
    "@superflycss/utilities-fonts": "^3.0.2",
    "@superflycss/utilities-layout": "^2.0.19",
    "@superflycss/utilities-format": "^1.0.0",
    "gulp": "^3.9.1",
    "lite-server": "^2.3.0",
    "npm-check-updates": "^2.12.1",
    "onchange": "^3.2.1"
  },
  "dependencies": {
    "@superflycss/variables-colors": "^2.1.0",
    "@superflycss/variables-dimension": "^1.0.1"
  }
}
